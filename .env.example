GH_TOKEN=
OPENAI_API_KEY=
OPENAI_MODEL=o1
MAX_PATCH_SIZE=10000

# Database configuration
# DB_TYPE: "sqlite" or "csv"
DB_TYPE=sqlite
# DB_NAME: Database filename (without extension)
# If relative path (default), stores in ~/.pretty-release-notes/
# If absolute path, stores at that exact location
DB_NAME=stored_lines

DEFAULT_OWNER=frappe
EXCLUDE_PR_TYPES=chore,refactor,ci,style,test
EXCLUDE_PR_LABELS=skip-release-notes
EXCLUDE_AUTHORS="mergify[bot],copilot-pull-request-reviewer[bot],coderabbitai[bot],dependabot[bot],cursor[bot]"
